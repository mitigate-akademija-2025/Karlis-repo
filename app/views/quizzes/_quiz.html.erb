

<div id="<%= dom_id quiz %>">
  <p>
    <strong>Title:</strong>
    <%= quiz.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= quiz.description %>
  </p>

  <p>
    <strong>Made by:</strong>
    <%= quiz.user ? quiz.user.username : "Unknown" %>
  </p>

  <%= link_to 'New question', new_quiz_question_path(quiz) %>
</div>
